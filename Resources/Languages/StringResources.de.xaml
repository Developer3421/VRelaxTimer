    <sys:String x:Key="ReportSavedMessage">Bericht wurde erfolgreich gespeichert.</sys:String>
    <sys:String x:Key="ReportSavedTitle">Erfolg</sys:String>
    <sys:String x:Key="ReportSaveError">Fehler beim Speichern des Berichts:</sys:String>
    <sys:String x:Key="Error">Fehler</sys:String>
